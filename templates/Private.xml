<templateSet group="Private">
  <template name="Test" value="@org.junit.jupiter.api.Test&#10;@org.junit.jupiter.api.DisplayName(&quot;$TEST_NAME$&quot;)&#10;void $METHOD_NAME$() {&#10;    $BODY$&#10;}" description="JUnit 5 Test Method" toReformat="false" toShortenFQNames="true">
    <variable name="TEST_NAME" expression="" defaultValue="&quot;Test name&quot;" alwaysStopAt="true" />
    <variable name="METHOD_NAME" expression="cameCase(TEST_NAME)" defaultValue="&quot;methodName&quot;" alwaysStopAt="true" />
    <variable name="BODY" expression="" defaultValue="&quot;org.junit.jupiter.api.Assertions.fail(\&quot;Not implemented\&quot;);&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
</templateSet>